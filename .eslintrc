{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended"],
  "rules": {
    "arrow-parens": ["error", "always"],
    "eqeqeq": ["error", "always"],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "linebreak-style": ["error", "unix"],
    "max-len": ["error", 80],
    "object-curly-spacing": ["error", "always"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"]
  }
}
